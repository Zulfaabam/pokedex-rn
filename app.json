{
  "expo": {
    "name": "pokedex-rn",
    "slug": "pokedex-rn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "src/assets/images/pokeball.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "pokedexrn",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-asset",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "src/assets/images/pokeball.png"
        }
      ]
    ]
  }
}
